<%= form_for @event, html: { autocomplete: :off } do |f| %>
  <div class="row">
    <div class="large-12 columns">
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
  </div>
  <div class="row">
    <div class="large-6 columns">

      <%= f.label :venue %>
      <%= f.select :venue, options_for_select(%w[Home Indoor Outdoor Multiple]) %> 
    </div>
    <div class="large-6 columns">
      <%= f.label :event_type %>
      <%= f.select :event_type, options_for_select(%w[Adult's_Birthday Baby_Shower Bachelor/Bachelorette Child's_Birtday Graduation Holiday Housewarming Wedding Wedding_Shower Other]) %>
    </div>
  </div>
  <div>
    <%= f.submit :class => "small button" %>
  </div>
<% end %>